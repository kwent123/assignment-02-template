function sumOfNumbers(arrayOfNumbers) {
  
  const arrayLength = arrayOfNumbers.length;
  let sum = 0;
  for (i = 0; i < arrayLength; i++) {
    sum = sum + arrayOfNumbers[i];
    
  }
  return(sum);
  /*console.log(sumOfNumbers(sum))*/
  
}

let a = sumOfNumbers([1, 2, 3, 4, 5, 6, 7]);
    


console.log(a);
